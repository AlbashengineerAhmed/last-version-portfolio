d: 10;\r\n$z-modal: 100;\r\n\r\n.section{\r\n  padding: 6.5rem 0 2rem;\r\n}\r\n.section__title{\r\n  text-align: center;\r\n  font-size: $h1-font-size;\r\n  margin-bottom: $normal-font-size;\r\n\r\n}\r\n/*--------Variables-------*/\r\n$green-color: #6cff8d;\r\n$dark-green-color: #6cff8c7a;\r\n$red-color:#a600ff;\r\n$blue-color:#2196f3;\r\n$yellow-color:#500182;\r\n$darkish-blue:#00002a;\r\n$box-shadow-blue1:rgba(255, 255, 255, 0.266);\r\n$box-shadow-blue2:rgba(255, 255, 255, 0.613);\r\n\r\n.section__title::before{\r\n  content: attr(data-heading);\r\n  display:block;\r\n  font-size: $normal-font-size;\r\n  font-weight: $font-medium;\r\n  color: $skin-color;\r\n}\r\n/*=============== LAYOUT ===============*/\r\n.container{\r\n  max-width: 98%;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n.grid{\r\n  display: grid;\r\n}\r\n\r\n*{\r\n  margin: 0;\r\n  box-sizing: border-box;\r\n  &::after,\r\n  &::before{\r\n    box-sizing: border-box;\r\n  }\r\n  scroll-behavior: smooth;\r\n}\r\nhtml,\r\nbody{\r\n  position: relative;\r\n  font-family: 'Poppins', sans-serif;\r\n  line-height: 1.5;\r\n  background-color: $darkish-blue;\r\n  -webkit-font-smoothing:antialiased;\r\n  z-index: 0;\r\n  height: 100vh;\r\n}\r\nbutton,\r\ninput,\r\ntextarea,\r\nselect{\r\n  font: inherit;\r\n}\r\np,\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6{\r\n  overflow-wrap: break-word;\r\n}\r\na{\r\n  text-decoration: none;\r\n  color: inherit;\r\n}\r\nul{\r\n  list-style: none;\r\n}\r\nimg,\r\nvideo,\r\ncanvas,\r\nsvg {\r\n  max-width: 100%;\r\n}\r\n\r\n\r\n.section{\r\n  padding-left: 1rem;\r\n  padding-right: 1rem;\r\n  overflow: hidden;\r\n  min-height: 100vh;\r\n  padding-top: 1rem;\r\n  @media (min-width: 650px) {\r\n    padding-left: 5rem;\r\n  padding-right: 5rem;\r\n  }\r\n  &__header{\r\n    &-title{\r\n      font-weight: 400;\r\n      font-size: 17px;\r\n      color: #500182;\r\n    }\r\n    &-subtitle{\r\n      font-size: 27px;\r\n      font-weight: 700;\r\n    }\r\n  }\r\n}\r\n.section__title{\r\n  color: #fff;\r\n}\r\n.section__header-subtitle{\r\n  color: #fff;\r\n}","assets\\styles\\coolLinks.scss":"@import \"./base.scss\";\r\n.coolLink{\r\n  position: relative;\r\n  display: grid;\r\n  place-items: center;\r\n  overflow: hidden;\r\n  &::before{\r\n    content: \"\";\r\n    position: relative;\r\n    bottom: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 3px;\r\n    background: currentColor;\r\n    transform: scale3d(0,1,1);\r\n    transform-origin: right;\r\n    transition: transform 0.5s cubic-bezier(0.5,0.5,0.3,1);\r\n  }\r\n  &::after{\r\n    content: attr(data-text);\r\n    position: absolute;\r\n    top: 10%;\r\n    height: 100%;\r\n    transform: translate3d(150%,0,0);\r\n  }\r\n  &:hover::before{\r\n    transform: scale3d(1,1,1);\r\n    transform-origin: left;\r\n  }\r\n  &:hover::after{\r\n    transform: translate3d(0,0,0);\r\n  }\r\n  & span{\r\n    transition: transform 0.5s cubic-bezier(0.5,0.5,0.3,1);\r\n  }\r\n  &:hover span{\r\n    transform: translate3d(-150%,0,0);\r\n  }\r\n}\r\n.coolButton{\r\n  position: relative;\r\n  border: none;\r\n  cursor: pointer;\r\n  width: 175px;\r\n  height: 120px;\r\n  background: none;\r\n  color: chartreuse;\r\n  &::before,\r\n  &::after{\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n  &::before{\r\n    content: \"\";\r\n    background-color: #fff !important;\r\n    clip-path: path(\"M154.5,88.5 C131,113.5 62.5,110 30,89.5 C-2.5,69 -3.5,42 4.5,25.5 C12.5,9 33.5,-6 85,3.5 C136.5,13 178,63.5 154.5,88.5 Z\");\r\n    transition: clip-path 0.5s cubic-bezier(0.585,2.5,0.645,0.55);\r\n  }\r\n  &:hover::before{\r\n    clip-path: path(\"M143,77 C117,96 74,100.5 45.5,91.5 C17,82.5 -10.5,57 5.5,31.5 C21.5,6 79,-5.5 130.5,4 C182,13.5 169,58 143,77 Z\");\r\n  }\r\n  & span{\r\n    mix-blend-mode: difference;\r\n    font-weight: 500;\r\n  }\r\n  &::after{\r\n    content: \"\";\r\n    height: 85%;\r\n    width: 95%;\r\n    top: 6px;\r\n    border: 1px solid #fff;\r\n    border-radius: 58% 42% 55% 45% / 56% 45% 55% 44%;\r\n    transform: rotate(-20deg);\r\n    transition: transform 0.5s cubic-bezier(0.585,2.5,0.645,0.58);\r\n  }\r\n  &:hover::after{\r\n    transform: translate3d(0, -5px, 0);\r\n  }\r\n  @media (max-width: 800px) {\r\n    &{\r\n      transform: scale3d(0.7,0.7,0.7);\r\n      font-size: 23px;\r\n    }\r\n  }\r\n}\r\n.coloredIcon{\r\n  color: #fff;\r\n  font-size: 35px;\r\n  &:hover{\r\n    animation: coloredIconAnimation 0.2s infinite;\r\n  }\r\n  @keyframes coloredIconAnimation {\r\n    0%{\r\n      text-shadow: 5px 4px $red-color, -5px -6px $blue-color;\r\n    }\r\n    25%{\r\n      text-shadow: -5px -6px $red-color, 5px 4px $blue-color;\r\n    }\r\n    50%{\r\n      text-shadow: 5px -4px $red-color, -8px 4px $blue-color;\r\n    }\r\n    75%{\r\n      text-shadow: -8px -4px $red-color, -5px -4px $blue-color;\r\n    }\r\n    100%{\r\n      text-shadow: -5px 0px $red-color, 5px -4px $blue-color;\r\n    }\r\n  }\r\n\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c2994370b73ba5b871291fe1a4860677","cacheData":{"env":{}}}