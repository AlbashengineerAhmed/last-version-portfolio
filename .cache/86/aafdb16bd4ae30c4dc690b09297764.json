ed":{"line":333,"column":2}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":201,"column":8},"generated":{"line":334,"column":4}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":206,"column":2},"generated":{"line":338,"column":0}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":207,"column":4},"generated":{"line":339,"column":2}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":208,"column":6},"generated":{"line":340,"column":4}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":210,"column":4},"generated":{"line":342,"column":2}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":211,"column":6},"generated":{"line":343,"column":4}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":212,"column":6},"generated":{"line":344,"column":4}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":213,"column":6},"generated":{"line":345,"column":4}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":214,"column":6},"generated":{"line":346,"column":4}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":215,"column":6},"generated":{"line":347,"column":4}},{"source":"assets\\styles\\work.scss","name":null,"original":{"line":216,"column":6},"generated":{"line":348,"column":4}}],"sources":{"assets\\styles\\base.scss":"@import url('https://fonts.googleapis.com/css2?family=Orbitron:wght@500&family=Poppins:wght@600&display=swap');\r\n\r\n/* _variables.scss */\r\n\r\n$hue-color: 242;\r\n\r\n\r\n/* RGB Color Mode (for demo purposes) */\r\n$skin-color: #a600ff;\r\n$title-color: #fff;\r\n$text-color: #fff;\r\n$body-color: #fff;\r\n$box-color: #e66f82;\r\n\r\n/* Font and Typography */\r\n$body-font: 'Poppins', sans-serif;\r\n\r\n$biggest-font-size: 3rem;\r\n$h1-font-size: 2.25rem;\r\n$h2-font-size: 1.5rem;\r\n$h3-font-size: 1.25rem;\r\n$normal-font-size: 1rem;\r\n$small-font-size: .875rem;\r\n$smaller-font-size: .813rem;\r\n\r\n/* Font Weight */\r\n$font-medium: 500;\r\n$font-bold: 600;\r\n\r\n/* Margins Bottom */\r\n$mb-0-25: .25rem;\r\n$mb-0-5: .5rem;\r\n$mb-0-75: .75rem;\r\n$mb-1: 1rem;\r\n$mb-1-5: 1.5rem;\r\n$mb-2: 2rem;\r\n$mb-2-5: 2.5rem;\r\n$mb-3: 3rem;\r\n\r\n/* Z Index */\r\n$z-fixed: 10;\r\n$z-modal: 100;\r\n\r\n.section{\r\n  padding: 6.5rem 0 2rem;\r\n}\r\n.section__title{\r\n  text-align: center;\r\n  font-size: $h1-font-size;\r\n  margin-bottom: $normal-font-size;\r\n\r\n}\r\n/*--------Variables-------*/\r\n$green-color: #6cff8d;\r\n$dark-green-color: #6cff8c7a;\r\n$red-color:#a600ff;\r\n$blue-color:#2196f3;\r\n$yellow-color:#500182;\r\n$darkish-blue:#00002a;\r\n$box-shadow-blue1:rgba(255, 255, 255, 0.266);\r\n$box-shadow-blue2:rgba(255, 255, 255, 0.613);\r\n\r\n.section__title::before{\r\n  content: attr(data-heading);\r\n  display:block;\r\n  font-size: $normal-font-size;\r\n  font-weight: $font-medium;\r\n  color: $skin-color;\r\n}\r\n/*=============== LAYOUT ===============*/\r\n.container{\r\n  max-width: 98%;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n.grid{\r\n  display: grid;\r\n}\r\n\r\n*{\r\n  margin: 0;\r\n  box-sizing: border-box;\r\n  &::after,\r\n  &::before{\r\n    box-sizing: border-box;\r\n  }\r\n  scroll-behavior: smooth;\r\n}\r\nhtml,\r\nbody{\r\n  position: relative;\r\n  font-family: 'Poppins', sans-serif;\r\n  line-height: 1.5;\r\n  background-color: $darkish-blue;\r\n  -webkit-font-smoothing:antialiased;\r\n  z-index: 0;\r\n  height: 100vh;\r\n}\r\nbutton,\r\ninput,\r\ntextarea,\r\nselect{\r\n  font: inherit;\r\n}\r\np,\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6{\r\n  overflow-wrap: break-word;\r\n}\r\na{\r\n  text-decoration: none;\r\n  color: inherit;\r\n}\r\nul{\r\n  list-style: none;\r\n}\r\nimg,\r\nvideo,\r\ncanvas,\r\nsvg {\r\n  max-width: 100%;\r\n}\r\n\r\n\r\n.section{\r\n  padding-left: 1rem;\r\n  padding-right: 1rem;\r\n  overflow: hidden;\r\n  min-height: 100vh;\r\n  padding-top: 1rem;\r\n  @media (min-width: 650px) {\r\n    padding-left: 5rem;\r\n  padding-right: 5rem;\r\n  }\r\n  &__header{\r\n    &-title{\r\n      font-weight: 400;\r\n      font-size: 17px;\r\n      color: #500182;\r\n    }\r\n    &-subtitle{\r\n      font-size: 27px;\r\n      font-weight: 700;\r\n    }\r\n  }\r\n}\r\n.section__title{\r\n  color: #fff;\r\n}\r\n.section__header-subtitle{\r\n  color: #fff;\r\n}","assets\\styles\\work.scss":"@import \"./base.scss\";\r\n/*===== Work Section =====*/\r\n.work__container {\r\n  grid-template-columns: repeat(4, 1fr);\r\n  gap: 1.8rem;\r\n  justify-content: center;\r\n  padding-top: 1rem;\r\n  @media (min-width: 1024px) {\r\n    grid-template-columns: repeat(3,1fr);\r\n  }\r\n  @media (max-width: 1200px) {\r\n    grid-template-columns: repeat(2,1fr);\r\n  }\r\n  @media (max-width: 1100px) {\r\n    grid-template-columns: repeat(2x,1fr);\r\n  }\r\n  @media (max-width: 867px) {\r\n    grid-template-columns: repeat(1,1fr);\r\n  }\r\n}\r\n.work{\r\n  background: linear-gradient(to right, #ff9100,#500182 );  \r\n}\r\n.work__filter {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  column-gap: 0.75rem;\r\n  margin-bottom: 2rem;\r\n  color: #fff;\r\n}\r\n\r\n.work__item {\r\n  cursor: pointer;\r\n  padding: 0.25rem 0.75rem;\r\n  font-weight: $font-medium;\r\n  border-radius: 0.5rem;\r\n}\r\n\r\n.work__card {\r\n  background: linear-gradient(to left, #ff9100,#500182 );  \r\n  padding: 1.25rem;\r\n  border-radius: 0.5rem;\r\n  color: $darkish-blue;\r\n}\r\n.work__container_img{\r\n  overflow: hidden;\r\n}\r\n.portfolio__item-details {\r\n  display: none;\r\n}\r\n\r\n.work__img {\r\n  border-radius: 0.5rem;\r\n  height: 90%;\r\n  transition: all 0.5s ease-in;\r\n  filter: blur(1px);\r\n}\r\n.work__card:hover .work__img{\r\n  transform: scale(1.1);\r\n  filter: brightness(1.5) blur(0px);\r\n}\r\n\r\n.work__title {\r\n  margin-top: 10px;\r\n  font-size: $normal-font-size;\r\n  font-weight: $font-medium;\r\n  margin-bottom: $mb-0-5;\r\n}\r\n\r\n.work__button {\r\n  color: $skin-color;\r\n  font-size: $small-font-size;\r\n  display: flex;\r\n  align-items: center;\r\n  column-gap: 0.25rem;\r\n  cursor: pointer;\r\n  &:hover {\r\n    .work__button-icon {\r\n      transform: translateX(0.25rem);\r\n    }\r\n  }\r\n}\r\n\r\n.work__button {\r\n  font-size: 1rem;\r\n  color: #fff;\r\n}\r\n\r\n.work__button-icon {\r\n  font-size: 1rem;\r\n  transition: 0.3s all ease-in;\r\n}\r\n\r\n/* Active Item Work */\r\n.active__work {\r\n  background-color: $skin-color;\r\n  color: $title-color;\r\n}\r\n\r\n/* Portfolio Popup */\r\n.portfolio__popup {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background-color: rgba(0, 0, 0, 0.7);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding: 0 1rem;\r\n  z-index: $z-modal;\r\n  transition: 0.4s all ease-in;\r\n  opacity: 0;\r\n  visibility: hidden;\r\n\r\n  &.open {\r\n    opacity: 1;\r\n    visibility: visible;\r\n  }\r\n}\r\n\r\n.portfolio__popup-inner {\r\n  // background-color: $darkish-blue;\r\n  background: linear-gradient(to right, #ff9100,#500182 );  \r\n  color: #fff;\r\n  width: 1000px;\r\n  height: 600px;\r\n  border-radius: 0.5rem;\r\n  padding: 2.5rem;\r\n  position: relative;\r\n}\r\n\r\n.portfolio__popup-content {\r\n  height: 100%;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  align-items: center;\r\n  column-gap: 3rem;\r\n}\r\n\r\n.portfolio__popup-close {\r\n  position: absolute;\r\n  top: 1.5rem;\r\n  right: 1.5rem;\r\n  font-size: 2rem;\r\n  // color: $skin-color;\r\n  color: #fff;\r\n  cursor: pointer;\r\n}\r\n.portfolio__popup-info{\r\n  margin: 15px;\r\n}\r\n.portfolio__popup-img {\r\n  height: 100%;\r\n  border-radius: 0.5rem;\r\n}\r\n\r\n.portfolio__popup-subtitle {\r\n  font-size: $small-font-size;\r\n  margin-bottom: $mb-0-25;\r\n}\r\n\r\n.details_title {\r\n  font-size: $h3-font-size;\r\n  font-weight: $font-medium;\r\n  margin-bottom: $mb-1;\r\n}\r\n\r\n.details_description {\r\n  font-size: $small-font-size;\r\n  margin-bottom: $mb-2;\r\n}\r\n.details__info{\r\n  padding: 0;\r\n}\r\n.details__info li {\r\n  margin-bottom: $mb-0-75;\r\n  text-transform: capitalize;\r\n  font-size: $small-font-size;\r\n\r\n  &:last-child {\r\n    margin-bottom: 0;\r\n  }\r\n\r\n  span {\r\n    font-weight: normal;\r\n  }\r\n\r\n  a {\r\n    text-transform: lowercase;\r\n    color: $skin-color;\r\n  }\r\n}\r\n  .portfolio__popup-content{\r\n    grid-template-columns: repeat(2, 1fr);\r\n    gap: 1.8rem;\r\n    @media (max-width: 876px) {\r\n      grid-template-columns: repeat(1,1fr);\r\n      &.portfolio__popup-inner{\r\n        height: auto !important;\r\n      }\r\n    }\r\n  }\r\n  \r\n  @media (max-width:876px) {\r\n    .portfolio__popup-inner{\r\n      height: auto !important;\r\n    }\r\n    .portfolio__popup-close {\r\n      position: absolute;\r\n      top: 0.5rem;\r\n      right: 0.5rem;\r\n      font-size: 2rem;\r\n      color: #fff;\r\n      cursor: pointer;\r\n    }\r\n  }\r\n"},"lineCount":null}},"error":null,"hash":"4b6160d7bb41b3cd9faca02a805152f3","cacheData":{"env":{}}}