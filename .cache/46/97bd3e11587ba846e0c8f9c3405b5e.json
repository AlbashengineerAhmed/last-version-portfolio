nherit;\r\n}\r\nul{\r\n  list-style: none;\r\n}\r\nimg,\r\nvideo,\r\ncanvas,\r\nsvg {\r\n  max-width: 100%;\r\n}\r\n\r\n\r\n.section{\r\n  padding-left: 1rem;\r\n  padding-right: 1rem;\r\n  overflow: hidden;\r\n  min-height: 100vh;\r\n  padding-top: 1rem;\r\n  @media (min-width: 650px) {\r\n    padding-left: 5rem;\r\n  padding-right: 5rem;\r\n  }\r\n  &__header{\r\n    &-title{\r\n      font-weight: 400;\r\n      font-size: 17px;\r\n      color: #500182;\r\n    }\r\n    &-subtitle{\r\n      font-size: 27px;\r\n      font-weight: 700;\r\n    }\r\n  }\r\n}\r\n.section__title{\r\n  color: #fff;\r\n}\r\n.section__header-subtitle{\r\n  color: #fff;\r\n}","assets\\styles\\projects.scss":"@import \"./base.scss\";\r\n.projects {\r\n  background: white;\r\n}\r\n.project {\r\n  &__header {\r\n    font-size: 60px;\r\n    font-style: italic;\r\n    padding-bottom: 10px;\r\n  }\r\n}\r\n\r\n.project {\r\n  &__header {\r\n    display: grid;\r\n    grid-template-columns: 1fr 3fr;\r\n    gap: 1rem;\r\n    font-family: sans-serif;\r\n    span {\r\n      border-top: 2px solid #03035b;\r\n      padding: 1rem;\r\n      font-size: 30px;\r\n      text-align: center;\r\n    }\r\n  }\r\n  &__infos {\r\n    margin-top: 1rem;\r\n    &--name {\r\n      position: relative;\r\n      font-size: 3vw;\r\n      font-family: sans-serif;\r\n      display: flex;\r\n      align-items: center;\r\n      font-weight: 400;\r\n    }\r\n  }\r\n  &__img {\r\n    position: relative;\r\n    width: 100vw;\r\n    height: 700px;\r\n    img {\r\n      width: 60vw;\r\n      height: 600px;\r\n      object-fit: cover;\r\n      position: absolute;\r\n      right: 10vw;\r\n      top: 3rem;\r\n    }\r\n  }\r\n  &__links {\r\n    padding-top: 3rem;\r\n    button {\r\n      &:before {\r\n        background-color: #ffa600;\r\n      }\r\n      &:after {\r\n        display: none;\r\n      }\r\n    }\r\n    .coolButton{\r\n      span{\r\n        color: #ffa600;\r\n      }\r\n    }\r\n  }\r\n  &__tags{\r\n    position: absolute;